{
  "name": "alchezoomy",
  "version": "1.0.0",
  "description": "#### Continuing the work of AlcheZOOMy",
  "main": "server.js",
  "scripts": {
    "test": "jest --verbose --runInBand",
    "test:watch": "npm run test -- --watch",
    "start": "node server.js",
    "start:watch": "nodemon server.js",
    "lint": "eslint .",
    "create-tables": "node data/create-tables.js",
    "drop-tables": "node data/drop-tables.js",
    "recreate-tables": "npm run drop-tables && npm run create-tables",
    "setup-db": "npm run recreate-tables",
    "setup-db:prod": "heroku run npm run setup-db",
    "setup-heroku": "heroku addons:create heroku-postgresql:hobby-dev && heroku config:set PGSSLMODE=require && heroku config:get DATABASE_URL"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "eslint": "^7.13.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "supertest": "^6.0.1"
  },
  "dependencies": {
    "aws-sdk": "^2.815.0",
    "bcryptjs": "^2.4.3",
    "colorthief": "^2.3.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.1",
    "node-webvtt": "^1.9.3",
    "nodemailer": "^6.4.18",
    "pg": "^8.5.1",
    "pg-format": "^1.0.4",
    "pg-promise": "^10.8.0",
    "superagent": "^6.1.0",
    "timezone": "^1.0.23"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlcheZoomy-2-0/zoom-search-sql.git"
  },
